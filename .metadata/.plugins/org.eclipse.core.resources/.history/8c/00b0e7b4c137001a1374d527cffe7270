package br.com.main;

import ex4.Gerente;
import ex4.Supervisor;
import ex4.Vendedor;

public class testeex4 {

	public static void main(String[] args) {
		 Gerente g1 = new Gerente("Pedro",20,4000);
		 Supervisor s1 = new Supervisor("Renan",29,4000);
		 Vendedor v1 = new Vendedor("carol",26,3800);

		 g1.status();
		 s1.status();
		 v1.status();
		 
		 
		 
		 g1.bonificação(2000);
		 s1.bonificação(1000);
		 v1.bonificação(500);
		 
		 g1.status();
		 s1.status();
		 v1.status();
		 
		 
		 
	}

}
