package ex4;

public class Vendedor extends Gerente {
	
	
	public Vendedor() {
		super();
		
	}

	public Vendedor(String nome, int idade, double salario) {
		super(nome, idade, salario);
		
	}

	public void bonificação(double sal) {
		super.bonificação(sal);
		this.salario += 300000; 
	}

	@Override
	public void status() {
		super.status();
	}
	
	
}
